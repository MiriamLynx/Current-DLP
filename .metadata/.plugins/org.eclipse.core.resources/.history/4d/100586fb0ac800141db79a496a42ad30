package ast.visitor;

import ast.Programa;
import ast.declaracion.Declaracion;
import ast.expresion.Expresion;
import ast.sentencia.Sentencia;

public interface Visitor {

	public Object visit(Programa programa);

	public Object visit(Declaracion declaracion);

	public Object visit(Expresion expresion);

	public Object visit(Sentencia sentencia);

}
