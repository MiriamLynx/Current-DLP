

public interface Token {
	
	public static final int CTE_ENTERA = 1;
	public static final int CTE_REAL = 2;
	public static final int CTE_CHAR = 3;
	public static final int CTE_STRING = 4;
	public static final int IDENTIFICADOR = 5;

}

Lexico lex;

Parser (Lexico lex){
	this.lex=lex;	
}

void yyerror(String s){
	System.out.println("Syntax error");
}

int yylex(){

	try{
		int token =lex.yylex();
		yylval=lex.lexeme();
		return token;
		}catch(Exception e){
		return -1;
		}
		}
