package ast.expresion;

import ast.tipo.Tipo;

public class AbstractExpresion implements AST {

	public Tipo tipo;
	public boolean lvalue;

	public Tipo getTipo() {
		return tipo;
	}

	public void setTipo(Tipo tipo) {
		this.tipo = tipo;
	}

	public boolean isLvalue() {
		return lvalue;
	}

	public void setLvalue(boolean lvalue) {
		this.lvalue = lvalue;
	}

}
