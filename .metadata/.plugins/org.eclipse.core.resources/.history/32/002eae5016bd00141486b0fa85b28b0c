package ast.declaracion;

import java.util.List;

import ast.AbstractAST;

public class DeclaracionStruct extends AbstractAST implements Declaracion {

	public String nombre;
	public List<DeclaracionVariable> Declaracioniniciones;

	public DeclaracionStruct(int linea, int columna, String nombre,
			List<DeclaracionVariable> Declaracioniniciones) {
		super(linea, columna);
		this.nombre = nombre;
		this.Declaracioniniciones = Declaracioniniciones;
	}

	@Override
	public String toString() {
		return "DeclaracionStruct [nombre=" + nombre
				+ ", Declaracioniniciones=" + Declaraciones + "]";
	}

}
