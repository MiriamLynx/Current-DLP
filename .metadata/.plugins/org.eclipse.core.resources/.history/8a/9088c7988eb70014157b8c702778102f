Lexico lex;

Parser (Lexico lex){
	this.lex=lex;	
}

void yyerror(String s){
	System.out.println("Syntax error");
}

int yylex(){

	try{
	
	int token =lex.yylex();
	yylval=lex.lexeme();
	
		return token;
		
	}catch(Exception e){
		return -1;
	}
}