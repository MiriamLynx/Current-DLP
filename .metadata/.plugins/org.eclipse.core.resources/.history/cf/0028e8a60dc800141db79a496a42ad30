package ast.visitor;

import ast.Programa;
import ast.declaracion.DeclaracionCampo;
import ast.declaracion.DeclaracionFuncion;
import ast.declaracion.DeclaracionStruct;
import ast.declaracion.DeclaracionVariable;
import ast.expresion.AccesoArray;
import ast.expresion.AccesoCampo;
import ast.expresion.Cast;
import ast.expresion.Comparacion;
import ast.expresion.ConstanteChar;
import ast.expresion.ConstanteEntera;
import ast.expresion.ConstanteReal;
import ast.expresion.LlamadaFuncion;
import ast.expresion.OperacionAritmetica;
import ast.expresion.OperacionLogica;
import ast.expresion.Variable;
import ast.sentencia.Asignacion;
import ast.sentencia.If;
import ast.sentencia.LlamadaFuncionSent;
import ast.sentencia.Print;
import ast.sentencia.Read;
import ast.sentencia.Return;
import ast.sentencia.While;
import ast.tipo.TipoArray;
import ast.tipo.TipoChar;
import ast.tipo.TipoEntero;
import ast.tipo.TipoError;
import ast.tipo.TipoReal;
import ast.tipo.TipoStruct;

public interface Visitor {

	public Object visit(Programa programa);

	public void visit(DeclaracionCampo declaracionCampo);

	public void visit(DeclaracionFuncion declaracionFuncion);

	public void visit(DeclaracionStruct declaracionStruct);

	public void visit(DeclaracionVariable declaracionVariable);

	public void visit(AccesoArray accesoArray);

	public void visit(AccesoCampo accesoCampo);

	public void visit(Cast cast);

	public void visit(Comparacion comparacion);

	public void visit(ConstanteChar constanteChar);

	public void visit(ConstanteEntera constanteEntera);

	public void visit(ConstanteReal constanteReal);

	public void visit(LlamadaFuncion llamadaFuncion);

	public void visit(OperacionAritmetica operacionAritmetica);

	public void visit(OperacionLogica operacionLogica);

	public void visit(Variable variable);

	public void visit(Asignacion asignacion);

	public void visit(If if1);

	public void visit(LlamadaFuncionSent llamadaFuncionSent);

	public void visit(Print print);

	public void visit(Read read);

	public void visit(Return return1);

	public void visit(While while1);

	public void visit(TipoArray tipoArray);

	public void visit(TipoChar tipoChar);

	public void visit(TipoEntero tipoEntero);

	public void visit(TipoError tipoError);

	public void visit(TipoReal tipoReal);

	public void visit(TipoStruct tipoStruct);

}
