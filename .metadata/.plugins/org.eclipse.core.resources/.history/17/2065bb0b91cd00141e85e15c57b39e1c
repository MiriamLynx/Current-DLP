package ast.tipo;

import ast.AST;
import ast.AbstractAST;
import ast.visitor.Visitor;

public class TipoError extends AbstractAST implements Tipo {

	public AST nodo;
	public String error;

	public TipoError(int linea, int columna, AST nodo, String error) {
		super(linea, columna);
		this.nodo = nodo;
		this.error = error;
	}

	public void accept(Visitor visitor) {
		visitor.visit(this);
	}

}