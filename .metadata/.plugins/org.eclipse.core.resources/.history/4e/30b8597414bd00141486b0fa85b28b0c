package ast.declaracion;

import java.util.List;

import ast.sentencia.Sentencia;
import ast.tipo.Tipo;

public class DeclaracionFuncion implements Declaracion {

	public Tipo retorno;
	public String nombre;
	public List<DeclaracionVariable> parametros;
	public List<DeclaracionVariable> definiciones;
	public List<Sentencia> sentencias;

	public DeclaracionFuncion(int linea, int columna, Tipo tipo, String nombre,
			List<DeclaracionVariable> parametros,
			List<DefVariable> definiciones, List<Sentencia> sentencias) {
		super(linea, columna);
		this.tipo = tipo;
		this.nombre = nombre;
		this.parametros = parametros;
		this.definiciones = definiciones;
		this.sentencias = sentencias;
	}

}
