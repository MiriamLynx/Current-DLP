package ast;

import java.util.List;

import ast.declaracion.DeclaracionVariable;
import ast.sentencia.Sentencia;

public class Programa extends AbstractAST {

	public List<Declaracion> declaraciones;

	public Programa(int linea, int columna,
			List<DeclaracionVariable> variables,
			List<DeclaracionVariable> definiciones, List<Sentencia> sentencias) {
		super(linea, columna);
		this.declaraciones = declaraciones;
	}

}
